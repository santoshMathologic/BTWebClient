    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            //"program": "${workspaceRoot}\\bin\\www"
            "program": "${workspaceRoot}/app.js"
        },
         {
            "type": "node",
            "request": "launch",
            "name": "Gulp Debugging Program",
            "program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
            "stopOnEntry": false,
            "args": ["start"],
            "cwd": "${workspaceRoot}",
             "preLaunchTask": null,
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--nolazy"
            ],
            "env": {
                "NODE_ENV": "development"
            },
            
            "sourceMaps": false,
            
            
        }
    ]
}